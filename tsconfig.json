{
 "compilerOptions": {
  "lib": [
   "es7",
   "dom"
  ],
  "target": "es5",
  "module": "commonjs",
  "jsx": "react",
  "noImplicitAny": true,
  "experimentalDecorators": true,
  "allowUnreachableCode": false,
  "strictNullChecks": true,
  "sourceMap": true,
  "noFallthroughCasesInSwitch": true
 },
 "exclude": [
  "node_modules",
  "typings",
  "_build",
  "nerves"
 ],
 "compileOnSave": false,
 "buildOnSave": false
}
